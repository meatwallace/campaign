{
  "name": "scripts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/_scripts",
  "projectType": "library",
  "targets": {
    "start-pg-test-container": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsx --tsconfig ./tsconfig.json ./start-pg-test-container.ts"
        ],
        "cwd": "projects/_scripts"
      }
    },
    "stop-pg-test-container": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsx --tsconfig ./tsconfig.json ./stop-pg-test-container.ts"
        ],
        "cwd": "projects/_scripts"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "tsc -p tsconfig.json --noEmit",
            "forwardAllArgs": false
          }
        ],
        "cwd": "projects/_scripts"
      }
    }
  },
  "tags": []
}
